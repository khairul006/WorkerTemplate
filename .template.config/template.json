{
    "$schema": "http://json.schemastore.org/template",
    "author": "zamidi.zakaria@teras.com.my",
    "classifications": [
        "Worker",
        "Template"
    ],
    "identity": "TERAS.WorkerTemplate",
    "name": "TERAS Worker Template",
    "shortName": "WorkerTemplate",
    "sourceName": "WorkerTemplate",
    "preferNameDirectory": true,
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sources": [
        {
            "source": "./",
            "target": "./",
            "exclude": [
                "bin/**",
                "obj/**",
                ".vs/**",
                ".git/**",
                "**/*.user",
                "**/*.suo",
                "Logs/**"
            ]
        }
    ],
    "symbols": {
        "userSecretsGuid": {
            "type": "generated",
            "generator": "guid",
            "replaces": "$guid1$"
        }
    }
}