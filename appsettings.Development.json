{
  "RabbitMQ": {
    "ConsumeVirtualHost": "/vhost",
    "ConsumeBrokerHost": "127.0.0.1",
    "ConsumeBrokerPort": 5672,
    "ConsumeBrokerUsername": "user",
    "ConsumeBrokerPassword": "pass",
    "ConsumeBrokerQueue": "queue",
    "ConsumeBrokerQueueType": "quorum",
    "ConsumeBrokerExchange": "",
    "ConsumeBrokerRK": "",
    "GlobalQOS": false,
    "Prefetch": 10
  },
  "PostgreSQL": {
    "Host": "127.0.0.1",
    "Port": "5432",
    "Username": "user",
    "Password": "pass",
    "Database": "db",
    "SslMode": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/worker-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
